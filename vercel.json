{
    "version": 2,
    "name": "music-album",
    "builds": [
       { 
        "src": "index.js", 
        "use": "@vercel/node" 
    }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "/index.js" 
        },
        {
            "src":"/api/private/(.*)",
            "dest":"/Routes/private.js"
        },
        {
            "src":"/bulletin/update/(.*)",
            "dest":"/Routes/Update.js"
        },
        {
            "src":"/bulletin/read/(.*)",
            "dest":"/Routes/Read.js"
        },
        {
            "src":"/bulletin/create/(.*)",
            "dest":"/Routes/Create.js"
        }
    ]
 }