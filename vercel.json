{
    "version": 2,
    "builds": [
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
        
    ],
    "routes": [
        {
            "src":"/(.*)",
            "destination":"/"
        },
        {
            "source": "/api/auth/:path*",
            "destination": "/:path*"
        }
    ]
}