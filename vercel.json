{
    "version": 3,
    "builds": [
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "rewrites": [
        {
            "source":"/api/auth/register",
            "destination":"/Controllers/auth"
        }
    ]
}